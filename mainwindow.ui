   <string notr="true">QWidget {
    background-color: #e6f7f5; /* light greenish background */
    color: #003366; /* dark blue text */
    font-size: 14px;
}

QPushButton {
    background-color: #4CAF50; /* green button */
    color: white;
    border: none;
    padding: 6px 12px;
    border-radius: 6px;
}

QPushButton:hover {
    background-color: #45a049; /* darker green on hover */
}

QLineEdit, QTextEdit {
    background-color: #ffffff;
    border: 2px solid #3399ff; /* blue border */
    border-radius: 4px;
    padding: 4px;
}

QLineEdit:focus, QTextEdit:focus {
    border: 2px solid #0066cc; /* darker blue when focused */
}

QLabel {
    color: #004d66; /* teal text */
    font-weight: bold;
}
QTabWidget::pane {
    border: 2px solid #3399ff; /* blue border */
    border-radius: 6px;
    background: #e6f7f5; /* light greenish background */
    padding: 4px;
}

QTabBar::tab {
    background: #4CAF50; /* green tabs */
    color: white;
    border: 1px solid #3399ff; /* blue border */
    border-radius: 6px;
    padding: 6px 12px;
    margin-right: 2px;
}

QTabBar::tab:selected {
    background: #3399ff; /* blue when selected */
    color: white;
}

QTabBar::tab:hover {
    background: #45a049; /* darker green hover */
}
QListView {
    background-color: #ffffff;
    border: 2px solid #3399ff; /* blue border */
    border-radius: 6px;
    padding: 4px;
    show-decoration-selected: 1; /* highlight whole row */
}

QListView::item {
    padding: 6px;
    border-bottom: 1px solid #e6f7f5; /* subtle divider */
    color: #003366; /* dark blue text */
}

QListView::item:selected {
    background-color: #4CAF50; /* green selection */
    color: white;
    border: none;
}

QListView::item:hover {
    background-color: #cceeff; /* light blue hover */
    color: #004d66;
}</string>
